def decreeboard:emuferal:emuferal_b18_devtest7.message.text
	{
		"templateClass": "LocalizationTemplate",
		"templateFormatHash": "392f1dafc8ef6ce858720cbef5e34ebc",
		"components": [
			{
				"componentClass": "LocalizationDefComponent",
				"componentJSON": {
					"needsTranslation": true,
					"en": "The Beta 1.8 Testing Build 7 is here! This is the final closed dev test build before we go into the application-based Early Access period of Beta 1.8!\n\nChanges in this dev test build:\n - Fixed chat formatting incorrectly rendering backslashes\n - Fixed the broken scale slider, it should no longer jump from min to max.\n - Fixed teleport animations randomly failing when players join the game\n - Fixed the Discord RPC mod crashing the game on DevTest.\n - Fixed incorrect placement of dragon head spark\n - Fixed asset bundles sometimes loading before FTL injects into them\n - Implemented closin of eyes during sleep animation\n - Reimplemented NPC head rotation\n - Added a 'no legs' option for dragons\n - Fixed camera zoom and center axis for creatures larger than OG max\n - Fixed offline unread counter not working\n - Added a public chat unread counter\n - Corrected the unread message count of the unread labels in chat buttons\n - Fixed chat window resizing glitching group message contents\n - Fixed scrolling of inactive chat tabs\n - Implemented a 'typing' status in chat\n - Improved chat bubble logic, it scrolls messages now\n - Fixed the favorite 'Player has joined you' spam bug\n - Added the Eat, Scared, Yes and No emotes for 1.8 clients\n\n - EmuFeral/Centuria Development Team",
					"fr": "",
					"de": "",
					"es": "",
					"pt": ""
				}
			},
			{
				"componentClass": "OldLocalizationDefComponent",
				"componentJSON": {}
			}
		]
	}
enddef

